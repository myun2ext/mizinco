= form_for @youtube do |f|
  - if @youtube.errors.any?
    #error_explanation
      %h2= "#{pluralize(@youtube.errors.count, "error")} prohibited this youtube from being saved:"
      %ul
        - @youtube.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :url
    = f.text_field :url
  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :hash
    = f.text_field :hash
  .actions
    = f.submit 'Save'
